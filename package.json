{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a secret-family-recipes6",
    "rollbackh": "heroku run knex migrate:rollback -a secret-family-recipes6",
    "databaseh": "heroku pg:psql -a secret-family-recipes6",
    "seedh": "heroku run knex seed:run -a secret-family-recipes6",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "14.16.0",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.7",
    "pg": "8.6.0",
    "knex-cleaner": "1.3.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "jest": "27.0.6",
    "jwt-decode": "^3.1.2",
    "nodemon": "2.0.12",
    "supertest": "6.1.4"
  }
}
